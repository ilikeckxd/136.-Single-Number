class Solution(object):
    def singleNumber(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        nums.sort()
        i = nums[0]
        while nums.count(i) == 2:
            nums.remove(nums[0])
            nums.remove(nums[0])
            i = nums[0]     
        return i
